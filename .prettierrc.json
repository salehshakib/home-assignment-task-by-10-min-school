{
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/config(/.*)?$",
    "^@/auth(/.*)?$",
    "^@/api(/.*)?$",
    "^@/lib(/.*)?$",
    "^@/store(/.*)?$",
    "^@/hooks(/.*)?$",
    "^@/constants(/.*)?$",
    "^@/app(/.*)?$",
    "",
    "^@/components/ui(/.*)?$",
    "",
    "^@/components(/.*)?$",
    "^[./]_components(/.*)?$",
    "^@/styles(/.*)?$",
    "",
    "^[./]",
    "",
    "^@/types(/.*)?$",
    "^types$",
    "<TYPES>",
    "<TYPES>^[./]",
    "",
    "^[.]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx"],
  "plugins": ["prettier-plugin-sort-json", "@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "printWidth": 120,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5"
}
